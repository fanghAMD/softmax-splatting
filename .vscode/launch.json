    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Default 320x320",
            "type": "debugpy",
            "request": "launch",
            "program": "./main.py",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "lf",
                "--one",
                "./input/images/one.png",
                "--two",
                "./input/images/two.png",
                "--out",
                "./output/out.png"
            ]
        },
